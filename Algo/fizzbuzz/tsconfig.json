{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es6"
        ],
        "module": "esnext",
        "rootDir": "src",
        "moduleResolution": "node",
        "allowJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true
    }
}